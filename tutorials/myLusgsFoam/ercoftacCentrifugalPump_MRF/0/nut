FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    INLET
    {
        type            calculated;
	value $internalField;
    }
    OUTLET
    {
        type            calculated;
        meanValue           0;
	value $internalField;
    }
    STATOR
    {
    	type		empty;
    }
    BLADE_ROT
    {
      type            nutkWallFunction;
      value $internalField;
    }
    BLADE_STAT
    {
      type            nutkWallFunction;
      value $internalField;
    }
    ROTOR 
    {
        type            empty;
    }
    AMI_INT
    {
        type            cyclicAMI;
    }   
    AMI_EXT
    {
        type            cyclicAMI;
    }   
    HUB_SHROUD
    {
        type            empty;
    }
}

// ************************************************************************* //
